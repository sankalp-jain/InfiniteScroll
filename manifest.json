{
  "manifest_version": 2,

  "name": "Infinite Scroll",
  "description": "An infinte scroll extension!",
  "version": "1.0",
  "icons": {
    "128": "logo.png",
    "48": "logo.png",
    "16": "logo.png"
  },

  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "permissions": [ "activeTab" ]
}